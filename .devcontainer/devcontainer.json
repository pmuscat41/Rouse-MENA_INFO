{
  "name": "jupyter-mapping-workspace",
  "dockerFile": "Dockerfile",
  "features": {
    "ghcr.io/devcontainers/features/python:1": {
      "version": "3.12"
    }
  },
  "postCreateCommand": "pip install -r requirements.txt",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-toolsai.jupyter",
        "ms-python.python"
      ]
    }
  },
  "forwardPorts": [8888],
  "remoteUser": "vscode"
}
